<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.afewsomethings.learnintent" >
    <uses-permission android:name="android.permission.INTERNET" />
    <user-permission android:name="android.permission.WRITE_SETTINGS"/>
    <user-permission android:name="android.permission.ACCOUNT_MANAGER"/>
    <user-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS"/>
    <user-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity_home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name="settingmenu"></activity>
        <activity android:name=".downloadbars"></activity>


    </application>

</manifest>
